triple(X,Y,Z) :- &rdf[A](X,Y,Z), inp(A), a(X), a(Y), a(Z).


knows(X,Y) :- triple(A, "http://xmlns.com/foaf/0.1/knows", B), name(A,X), name(B,Y).
name(X,Y) :- triple(X, "http://xmlns.com/foaf/0.1/name", Y).

inp(X) :- triple(Z,"http://www.w3.org/2000/01/rdf-schema#seeAlso",X).

inp(X) :- input(X).

input(X) :- url(X), not -input(X).
-input(X) :- url(X), input(Y), X != Y.

