% S is no proper subset of T
% (i) S is no subset of T at all
%isNoSubset :- input(s, X, X), not input(t, X, X).
%ok :- isNoSubset.
% (ii) S is no PROPER subset, i.e., every element of T is in S
%notEveryElementOfTIsInS :- input(t, X, X), not input(s, X, X).
%ok :- not isNoSubset, not notEveryElementOfTIsInS.


% S is a superset of T
noSuperSet :- input(s, X, X), not input(t, X, X).
ok :- not noSuperSet.

% S and T are incomparible
ok :- input(s, X, X), not input(t, X, X), not input(s, Y, Y), input(t, Y, Y).



% T is not admissible
% possibility 1: T is conflicting
ok :- input(t, X, X), input(t, Y, Y), input(edge, X, Y).
% possibility 2: not each a \in T is defended by T
isAttackedBy(X, Y) :- input(t, X, X), input(edge, Y, X).
isDefended(X) :- isAttackedBy(X, Y), input(t, Z, Z), input(edge, Z, Y).
ok :- isAttackedBy(X, Y), not isDefended(X).
